<h1>List#edit</h1>
<%= form_with model: @list,url:update_list_path(@list.id), method: :patch do |f| %>

<h2>Title</h2>
 <%= f.text_field :title %>

<h2>Body</h2>
 <%= f.text_area :body %>
 <%= f.submit 'Update Book' %>

<%= link_to "Edit", edit_list_path(@list.id) %>
<%= link_to "Back", "/lists" %>

<% end %>